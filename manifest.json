{
  "manifest_version": 2,
  "name": "Dark Mode Enabler",
  "version": "0.0.1",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": "assets/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"]
    }
  ],
  "background": {
    "scripts": []
  }
}
